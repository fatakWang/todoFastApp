{
  "package": "asddsa",
  "name": "da",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1070,
  "icon": "/assets/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.webview"
    },
    {
      "name": "service.share"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.vibrator"
    },
    {
      "name": "service.asr"
    },
    {
      "name": "system.storage"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "memo",
  "config": {
    "logLevel": "debug",
    "designWidth": 360
  },
  "router": {
    "entry": "pages/main",
    "pages": {
      "pages/main": {
        "component": "index"
      },
      "pages/add": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBar": true,
    "titleBarBackgroundColor": "#f4f6fa",
    "titleBarTextColor": "#333333",
    "menu": true,
    "pages": {
      "pages/main": {
        "titleBarText": "待办"
      },
      "pages/add": {
        "titleBar": false,
        "statusBarBackgroundColor": "@brand"
      }
    }
  }
}
